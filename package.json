{
  "name": "irecord-app",
  "id": "uk.ac.ceh.irecord",
  "title": "iRecord App",
  "description": "iRecord mobile application.",
  "version": "6.0.5",
  "homepage": "https://irecord.org.uk",
  "scripts": {
    "start": "webpack serve --progress --port 8001",
    "build": "NODE_ENV=production webpack",
    "build:production": "npm run clean && npm run build && npx cap sync && sed -i '' 's/uk.ac.ceh.irecord/uk.ac.ceh.ir/g' ios/App/App/capacitor.config.json && npm run build:resources",
    "build:resources": "mkdir -p resources && cp -R other/designs/android resources && cp -R other/designs/*.png resources && npx cordova-res ios --skip-config --resources resources --copy && npx cordova-res android --skip-config --resources resources --copy",
    "deploy": "cp other/demo.html build/ && URL=$(vercel --prod -t $VERCEL_TOKEN --confirm ./build) && CLEAN_URL=$(echo $URL | sed -r \"s/https:\\/\\/(.*)$/\\1/g\") && DEMO_URL=$(echo $CLEAN_URL | sed -r \"s/(.*\\-)[a-z0-9]+\\-flumens(\\.vercel\\.app)$/\\1${BITRISE_BUILD_NUMBER}\\2/g\") && vercel alias set $CLEAN_URL $DEMO_URL -t $VERCEL_TOKEN && envman add --key DEMO_URL --value \"https://$DEMO_URL\"",
    "deploy:production": "bitrise ci",
    "clean": "rm -rf build resources",
    "test": "jest --silent --passWithNoTests",
    "test:watch": "jest --watchAll",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --watch test",
    "test:eslint": "eslint src --quiet --ext .jsx --ext .ts --ext .tsx --ext .js && echo '\\033[37;42;1m PASS \\033[00m'",
    "make:species": "cd src/common/data && node make.js"
  },
  "dependencies": {
    "@capacitor-community/camera-preview": "^3.1.1",
    "@capacitor/android": "^4.4.0",
    "@capacitor/app": "^4.1.0",
    "@capacitor/camera": "^4.1.3",
    "@capacitor/core": "^4.4.0",
    "@capacitor/device": "^4.0.1",
    "@capacitor/filesystem": "^4.1.3",
    "@capacitor/geolocation": "^4.0.1",
    "@capacitor/haptics": "^4.0.1",
    "@capacitor/ios": "^4.4.0",
    "@capacitor/keyboard": "^4.0.1",
    "@capacitor/network": "^4.0.1",
    "@capacitor/splash-screen": "^4.1.1",
    "@capacitor/status-bar": "^4.0.1",
    "@changey/react-leaflet-markercluster": "^4.0.0-rc1",
    "@flumens/bigu": "^0.4.0",
    "@flumens/ionic": "1.20.2",
    "@ionic-native/core": "5.36.0",
    "@ionic-native/launch-review": "^5.29.0",
    "@ionic/core": "^6.2.6",
    "@ionic/react": "^6.2.6",
    "@ionic/react-router": "^6.2.6",
    "@sentry/browser": "^7.16.0",
    "@sentry/integrations": "^7.16.0",
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "cordova-launch-review": "^4.0.1",
    "cordova-plugin-device": "^2.1.0",
    "cordova-sqlite-storage": "6.0.0",
    "deep-diff": "^1.0.2",
    "formik": "2.2.9",
    "i18next": "^21.8.9",
    "ionicons": "^6.0.2",
    "json-stable-stringify": "^1.0.1",
    "jwt-decode": "^3",
    "leaflet": "1.7.1",
    "leaflet.markercluster": "^1.5.3",
    "localforage-cordovasqlitedriver": "1.8.0",
    "lodash.mergewith": "^4.6.2",
    "mapbox-gl": "2.4.1",
    "mobx": "^6.6.0",
    "mobx-react": "^7.5.0",
    "mobx-utils": ">=6",
    "mock-geolocation": "1.0.11",
    "proj4": "^2",
    "proj4leaflet": "^1",
    "prop-types": "15.8.1",
    "prop-types-exact": "1.2.0",
    "react": "^17.0.2",
    "react-autosuggest": "^10",
    "react-dom": "^17.0.2",
    "react-easy-crop": "^4.6.2",
    "react-i18next": "^11.17.1",
    "react-leaflet": "3.2.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-window": "^1.8.7",
    "swiper": "^8.2.2",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-exponentiation-operator": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@capacitor/cli": "^4.4.0",
    "@flumens/eslint-config": "^2.0.1",
    "@flumens/webpack-config": "^5.0.0",
    "@types/jest": "^27.5.1",
    "@types/leaflet": "^1.7.10",
    "@types/lodash.mergewith": "^4.6.7",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.5",
    "babel-eslint": "10.1.0",
    "babel-jest": "^28.1.0",
    "babel-plugin-module-resolver": "4.1.0",
    "cordova-res": "^0.15.4",
    "dotenv": "^16.0.1",
    "eslint": "^8.15.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-transform-stub": "2.0.0",
    "sinon": "^8.0.4",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vercel": "^24.2.3",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "author": {
    "name": "Flumens",
    "email": "enquiries@flumens.io",
    "url": "https://flumens.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NERC-CEH/irecord-app"
  },
  "bugs": {
    "url": "https://github.com/NERC-CEH/irecord-app/issues"
  },
  "licenses": [
    {
      "type": "GNU GPL v3",
      "url": "http://www.gnu.org/licenses/gpl.html"
    }
  ],
  "keywords": [
    "irecord",
    "biological",
    "recording",
    "mobile",
    "app",
    "offline",
    "ceh",
    "brc"
  ],
  "browserslist": [
    "Android >= 5.1",
    "iOS >= 10"
  ],
  "babel": {
    "assumptions": {
      "setPublicClassFields": true
    },
    "presets": [
      [
        "@babel/react",
        {
          "runtime": "automatic"
        }
      ],
      [
        "@babel/env",
        {
          "targets": {
            "android": "5.1",
            "ios": "10.2"
          },
          "loose": true,
          "modules": "commonjs",
          "corejs": "3.0.0",
          "useBuiltIns": "usage"
        }
      ],
      [
        "@babel/typescript",
        {
          "allowDeclareFields": true
        }
      ]
    ],
    "plugins": [
      [
        "@babel/transform-runtime",
        {
          "regenerator": true
        }
      ],
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@flumens$": "./src/common/flumens",
            "common": "./src/common",
            "helpers": "./src/common/helpers",
            "models": "./src/common/models",
            "Components": "./src/common/Components"
          }
        }
      ],
      [
        "@babel/proposal-private-property-in-object",
        {
          "loose": true
        }
      ],
      "@babel/transform-exponentiation-operator",
      "@babel/syntax-dynamic-import"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@flumens",
      "plugin:react/jsx-runtime"
    ],
    "rules": {
      "react/function-component-definition": 0,
      "arrow-body-style": 0,
      "camelcase": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "prefer-arrow-callback": 0,
      "react/no-unused-class-component-methods": 0,
      "sort-class-members/sort-class-members": 0,
      "react/prop-types": 0,
      "react/jsx-no-useless-fragment": 0,
      "@typescript-eslint/no-empty-function": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "react/jsx-no-bind": 0,
      "class-methods-use-this": 0,
      "@typescript-eslint/no-var-requires": 0,
      "no-restricted-exports": 0,
      "default-param-last": 0,
      "@typescript-eslint/no-this-alias": 0,
      "jest/valid-expect": 0,
      "jest/expect-expect": 0,
      "jest/no-done-callback": 0,
      "jest/no-conditional-expect": 0,
      "@getify/proper-arrows/name": 0
    },
    "globals": {
      "t": "readonly"
    },
    "ignorePatterns": [
      "**/*test.js"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "jsxBracketSameLine": false,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "babel-jest",
        {
          "presets": [
            [
              "@babel/env",
              {
                "loose": true
              }
            ]
          ]
        }
      ]
    },
    "testRegex": "src.*((\\.|/*.)(test))\\.(t|j)s?$",
    "moduleNameMapper": {
      "^.+.(css|style|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "ionic-native": "jest-transform-stub",
      "swiper": "jest-transform-stub"
    },
    "moduleDirectories": [
      "node_modules",
      "components",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@ionic|@stencil|ionicons)"
    ]
  }
}

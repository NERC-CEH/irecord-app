  "description": "iRecord mobile application.",
  "version": "4.0.0",
  "build": "1",
  "homepage": "https://irecord.org.uk",
  "scripts": {
    "build": "grunt",
    "build:update": "grunt udpate",
    "build:dev": "grunt dev",
    "build:species": "cd src/common/data && node species.js",
    "build:dev:update": "grunt dev:update",
    "build:cordova": "grunt cordova",
    "build:cordova:update": "grunt cordova:update",
    "build:cordova:android": "grunt cordova:android",
    "postinstall": "grunt dev",
    "start": "node other/server.js",
    "start-dev": "webpack-serve --config other/webpack.dev.js --content dist/main --port 8000",
    "test": "karma start test/karma.conf.js",
    "test:coverage": "karma start test/karma.conf.js --reporters mocha,coverage",
    "test:watch": "karma start test/karma.conf.js --no-single-run",
    "test:sauce": "karma start test/karma.conf-sauce.js",
    "test:eslint": "mocha test/eslint-test.js",
    "test:acceptance": "grunt exec:cordova_build_android && ./node_modules/.bin/wdio test/wdio.conf.js",
    "clean": "rm -rf node_modules dist yarn.lock package-lock.json",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test && npm run test:eslint"
    }
  },
